{
  "name": "i18n",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "node ./scripts/check-node-version.js",
    "ng": "ng",
    "start": "ng serve",
    "start:ar": "ng serve --configuration=ar-JO",
    "build": "ng build",
    "build:prod": "ng build --configuration=production --output-path=./dist",
    "build:en": "ng build --configuration=production-en",
    "build:ar": "ng build --configuration=production-ar",
    "build:i18n": "npm run build:en && npm run build:ar",
    "start:prod": "node dist/server/server.mjs",
    "extract-i18n": "ng extract-i18n",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "engines": {
    "node": "22.16.x"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.3",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/localize": "^20.0.5",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
